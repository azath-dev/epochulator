{
  "name": "Epochulator",
  "version": "0.1.0",
  "description": "Easy Epoch Date Manipulation",
  "permissions": ["declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/epochulator16.png",
      "32": "images/epochulator32.png",
      "48": "images/epochulator48.png",
      "128": "images/epochulator128.png"
    }
  },
  "icons": {
    "16": "images/epochulator16.png",
    "32": "images/epochulator32.png",
    "48": "images/epochulator48.png",
    "128": "images/epochulator128.png"
  },
  "manifest_version": 2
}
